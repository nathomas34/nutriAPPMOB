{
  "name": "angular-starter",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "serve:prod": "ng serve --configuration=production",
    "android:setup": "npx cap add android",
    "android:sync": "npm run build:prod && npx cap sync android",
    "android:open": "npx cap open android",
    "android:build": "npm run build:prod && npx cap sync android && npx cap open android",
    "ios:setup": "npx cap add ios",
    "ios:sync": "npm run build:prod && npx cap sync ios",
    "ios:open": "npx cap open ios",
    "deploy:netlify": "ng build --configuration=production && npx netlify deploy --prod --dir=dist/demo",
    "deploy:vercel": "ng build --configuration=production && npx vercel --prod"
  },
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@capacitor/android": "^6.1.2",
    "@capacitor/camera": "^7.0.1",
    "@capacitor/cli": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/device": "^7.0.1",
    "@capacitor/haptics": "^7.0.1",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/share": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@ionic/angular": "^8.7.0",
    "@ionic/angular-toolkit": "^12.2.0",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "ng2-charts": "^8.0.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "typescript": "^5.8.2"
  }
}
